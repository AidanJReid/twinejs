{
	"name": "Twine",
	"version": "2.4.0-alpha1",
	"description": "a GUI for creating nonlinear stories",
	"author": "Chris Klimas <chris@twinery.org>",
	"scripts": {
		"build": "npm-run-all -l --parallel build:cdn build:electron build:web --serial build:build-number",
		"test": "vue-cli-service test:unit --watch",
		"lint": "eslint src/ || exit 0",
		"build:build-number": "node scripts/build-number.js",
		"build:cdn": "cross-env NODE_ENV=production USE_CDN=y webpack",
		"build:electron": "cross-env NODE_ENV=production USE_ELECTRON=y webpack && electron-builder --ia32 --x64 -mwl",
		"build:electron-dev": "cross-env USE_ELECTRON=y webpack --progress && electron .",
		"build:web": "cross-env NODE_ENV=production webpack",
		"clean": "rimraf dist/",
		"i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
		"lint:noisy": "eslint src/",
		"pot": "node scripts/extract-pot.js",
		"prettier": "prettier --write \"src/**/*.{js,less}\"",
		"start": "vue-cli-service serve src/index.js",
		"storybook": "vue-cli-service storybook:serve --config-dir storybook --port 6006",
		"test:coverage": "vue-cli-service test:unit --coverage",
		"test:once": "vue-cli-service test:unit"
	},
	"main": "src/electron/index.js",
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.25",
		"@fortawesome/free-solid-svg-icons": "^5.11.2",
		"@fortawesome/vue-fontawesome": "^0.1.7",
		"blob-polyfill": "^1.0.20150320",
		"browser-saveas": "^1.0.0",
		"codemirror": "^5.5.0",
		"core-js": "^2.6.5",
		"detect-it": "^3.0.5",
		"fastclick": "^1.0.6",
		"font-awesome": "^4.3.0",
		"fs-extra": "^7.0.1",
		"jed": "^1.1.0",
		"jsonp": "^0.2.1",
		"jszip": "^2.5.0",
		"klaw-sync": "^6.0.0",
		"lodash.escape": "^4.0.1",
		"lodash.escaperegexp": "^4.1.2",
		"lodash.uniq": "^4.5.0",
		"lodash.values": "^4.3.0",
		"lodash.without": "^4.4.0",
		"mkdirp-promise": "^5.0.1",
		"moment": "^2.10.3",
		"scroll": "^2.0.0",
		"segseg": "^0.2.2",
		"semver-utils": "^1.1.1",
